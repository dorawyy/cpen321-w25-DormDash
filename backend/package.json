{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:mock": "node test-runner.js",
    "test:nomock": "node test-runner.js",
    "test:mock:coverage": "node test-runner.js",
    "test:nomock:coverage": "node test-runner.js",
    "test:mock:auth": "node test-runner.js",
    "test:mock:auth:coverage": "node test-runner.js",
    "test:mock:jobs": "node test-runner.js",
    "test:mock:jobs:coverage": "node test-runner.js",
    "test:mock:order": "node test-runner.js",
    "test:mock:order:coverage": "node test-runner.js",
    "test:mock:payment": "node test-runner.js",
    "test:mock:payment:coverage": "node test-runner.js",
    "test:mock:routePlanner": "node test-runner.js",
    "test:mock:routePlanner:coverage": "node test-runner.js",
    "test:mock:user": "node test-runner.js",
    "test:mock:user:coverage": "node test-runner.js",
    "test:nomock:auth": "node test-runner.js",
    "test:nomock:auth:coverage": "node test-runner.js",
    "test:nomock:jobs": "node test-runner.js",
    "test:nomock:jobs:coverage": "node test-runner.js",
    "test:nomock:order": "node test-runner.js",
    "test:nomock:order:coverage": "node test-runner.js",
    "test:nomock:payment": "node test-runner.js",
    "test:nomock:payment:coverage": "node test-runner.js",
    "test:nomock:routePlanner": "node test-runner.js",
    "test:nomock:routePlanner:coverage": "node test-runner.js",
    "test:nomock:user": "node test-runner.js",
    "test:nomock:user:coverage": "node test-runner.js",
    "update-test-scripts": "node update-test-scripts.js",
    "clear-jobs": "ts-node scripts/clear-jobs.ts",
    "load-test:seed-data": "ts-node tests/load-test/seed-data-locally.ts",
    "load-test:generate-tokens": "node tests/load-test/generate-tokens.js",
    "load-test:setup-local": "npm run load-test:seed-data && npm run load-test:generate-tokens",
    "load-test:cleanup-local": "ts-node tests/load-test/cleanup-data-locally.ts",
    "load-test:local": "npm run load-test:setup && k6 run tests/load-test/load-test.js && npm run load-test:cleanup",
    "load-test:setup": "ts-node tests/load-test/setup.ts",
    "load-test:cleanup": "ts-node tests/load-test/cleanup.ts",
    "load-test:no-cleanup": "npm run load-test:setup && k6 run tests/load-test/load-test.js",
    "load-test": "npm run load-test:setup && k6 run tests/load-test/load-test.js && npm run load-test:cleanup",
    "load-test:direct": "k6 run tests/load-test/load-test.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "seed-jobs": "node scripts/seed-test-jobs.js",
    "seed-jobs:ts": "ts-node scripts/seed-test-jobs.ts",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test:file": "jest $npm_config_file",
    "test:file:coverage": "jest $npm_config_file --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^19.1.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "maxWorkers": 1,
    "testTimeout": 30000,
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/*.d.ts",
      "!src/index.ts",
      "!src/socket.ts",
      "!src/config/**",
      "!src/utils/**",
      "!src/controllers/dev.controller.ts",
      "!src/controllers/loadTest.controller.ts",
      "!src/routes/dev.routes.ts",
      "!src/routes/loadTest.routes.ts"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/dist/"
    ]
  }
}